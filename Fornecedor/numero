#include <stdio.h>
void telefone(char *tele){
    char tele1[18];
    snprintf(tele1,sizeof(tele1),"(%c%c)%c%c%c%c%c-%c%c%c%c",
   tele[0],tele[1],
   tele[2],tele[3],tele[4],tele[5],tele[6],
   tele[7],tele[8],tele[9],tele[10]);
   printf("%s",tele1);
}
int main(){
   
    char numero[18];
    printf("escreva os numeros");
    scanf(" %s",numero);
    telefone(numero);
    
    return 0;
}
